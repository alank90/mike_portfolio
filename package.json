{
  "name": "rapo-browserify-budo",
  "version": "1.0.0",
  "description": "A barebones Browserify project to get started with Rapid (ra) Prototyping (po) our Web projects",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "budo index.js --live --open",
    "build": "node ./scripts/build",
    "testbuild": "budo -d ./build --open",
    "css-clean": "npx postcss src/css/*.css autoprefixer -d src/css && npx postcss src/css/*.css --replace --no-map"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peterbarraud/rapo-browserify-budo.git"
  },
  "keywords": [
    "browserify",
    "budo",
    "prototyping"
  ],
  "author": "@gapeterb",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/peterbarraud/rapo-browserify-budo/issues"
  },
  "homepage": "https://github.com/peterbarraud/rapo-browserify-budo#browserify-with-budo",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "budo": "^11.6.3",
    "copy-dir": "^1.2.0",
    "css-declaration-sorter": "^5.0.0",
    "file-copy": "^1.0.0",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "rimraf": "^3.0.1",
    "uglifycss": "0.0.29",
    "uglifyify": "^5.0.2"
  },
  "dependencies": {
    "mkdirp": "^1.0.3"
  }
}
